<%= simple_form_for(@boat ) do |form| %>
 <%= form.error_notification %>

  <div class="form-inputs">

<%= form.input :member_id, :as => :hidden %>
<%= form.input :boat_name %>
<%= form.input :boat_type , :as => :select, :collection => Boat.types  %>
<%= form.input :boat_class  %>
<%= form.input :sail_number %>
<%= form.input :pen_tag  %>

 </div>

  <div class="form-actions">

      <%= form.button :submit  %>
</div>
<% end %>

