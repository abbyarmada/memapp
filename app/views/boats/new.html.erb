<h1>New Boat</h1>
<% member_id = params[:member_id] %>

<%= link_to person_path(@boat.owner) + '#tabs-5', :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  
  <%= form_tag :action => 'create',:member_id => member_id do %>
    <%= render :partial => "form", :locals => { :button_label => "Create", :boat => @boat , :member_id => member_id } %>
  <% end %>
