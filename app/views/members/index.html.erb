<div class="page-header">
  <%= link_to new_member_path, :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Member
  <% end %>
  <h1>Listing members</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
         
            <th>Proposed</th>
            <th>Seconded</th>
            <th>Year joined</th>
         <th>Status</th>
      
            <th>Renew date</th>
            <th>Privilege</th>
            <th>Name no</th>
            <th>Street1</th>
           
            <th>City</th>
            
            <th>Country</th>
            <th>Email renewal</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @members) do |member| %>
        
            <td><%= member.proposed %></td>
            <td><%= member.seconded %></td>
            <td><%= member.year_joined %></td>
            <td><%= member.active %></td>
      
            
            <td><%= member.renew_date %></td>
            <td><%= member.privilege.name %></td>
            <td><%= member.name_no %></td>
            <td><%= member.street1 %></td>
           
            <td><%= member.city %></td>
           
            <td><%= member.country %></td>
            <td><%= member.email_renewal %></td>
            <td><%= link_to 'Show', member %></td>
      <td><%= link_to 'Edit', edit_member_path(member)  %></td>
        <td><%= link_to 'Delete', member, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-mini btn-danger' %></td>
      <% end %>
    </tbody>
  </table>
</div>
