<%= form_tag people_path, :method => 'get' ,:class => "form-inline" ,:id => "people_search"  do %>

<% if !params[:searchmp].blank? %>
	<% options = {:prompt => "-Select a Membership Class",:selected => params[:searchmp][:privilege_id]} %>
<% else %>
	<% options = {:prompt => "-Select a Membership Class"} %>
<% end %>
<div class="form-group">
  <%= text_field_tag :search, params[:search] ,:placeholder => 'Last Name' %>
  <%= text_field_tag :searchfn, params[:searchfn], :placeholder => 'First Name'%>

  <div class="form-group" id="2">
  <%= collection_select(:searchmp, :privilege_id, Privilege.all, :id,:name, options, :class =>"" ) %>
    <div class="checkbox">
      <%= check_box_tag :past_members,true,params[:past_members] %><%= label_tag(:past_members, " Past Members") %>
    </div >
    <div class="checkbox">
      <%= check_box_tag :group,true,params[:group] %><%= label_tag(:group, " Group Family Members") %>
    </div>
  </div>
</div>
<div class="form-group">
  <button type="submit" class="btn btn-primary", data-disable-with="Wait..." >Submit</button>
    <a class="btn btn-default" href="/" role="button"> Reset</a>
</div>
<% end %>
