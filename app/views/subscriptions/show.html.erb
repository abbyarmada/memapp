<div class="page-header">
  <%= link_to subscriptions_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_subscription_path(@subscription), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show subscription</h1>
</div>

<dl class="dl-horizontal">
  <dt>Amount:</dt>
  <dd><%= @subscription.amount %></dd>

  <dt>Start date:</dt>
  <dd><%= @subscription.start_date %></dd>

  <dt>End date:</dt>
  <dd><%= @subscription.end_date %></dd>

  <dt>Privilege:</dt>
  <dd><%= @subscription.privilege_id %></dd>

</dl>
